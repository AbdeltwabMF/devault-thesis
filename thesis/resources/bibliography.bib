%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Website																																				%
% For websites, note the specific formatting of the howpublished field. Be			%
% sure to include the date of access as shown. Also, keep in mind that certain	%
% characters common in urls, such as underscores, must be escaped (preceded			%
% by a backslash, ex.																														%
% Used if none of the other entry types quite match the source.     			      %
% Frequently used to cite web pages, but can be anything from lecture slides  	%
% to personal notes.                                                            %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
@misc{tetazoo05,
	author = {James Tetazoo},
	title = {A brief guide to recreational pyromania},
	howpublished = {Available at \url{http://
	www.blowinglotsofweirdstuffup.com/guide.html} (2005/06/12)}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Accepted for Publication																							%
% To show that an article has been accepted for publication but not yet	%
% published, include the line note = {(in press)}.											%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
@unpublished{mentorAcc,
	author = {J Mentor},
	title = {Behavior of small animals on fire},
	journal = {Nature},
	note = {(in press)}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Submitted for Publication																									%
% If a manuscript has been submitted but not yet accepted for publication,	%
% include a note to this effect.																						%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
@unpublished{mentorSub,
	author = {J Mentor},
	title = {Behavior of small animals on fire},
	note = {Manuscript submitted for publication},
	year = {2012}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Not Published																															%
% If a manuscript has not yet been published or submitted for publication,	%
% you may still cite it. Include a note to this effect.											%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
@unpublished{mentorUnp,
	author = {J Mentor},
	title = {Behavior of small animals on fire},
	note = {Unpublished Manuscript},
	year = {2012}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Conversation																																											%
% Occasionally, an important item that does not exist in the literature will												%
% be discussed with your mentor. When this happens, you may cite the conversation with your mentor.	%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
@misc{freud12,
	author = {Sigmund Freud},
	month = {July},
	year = {2012},
	howpublished = {Personal conversation}
}

